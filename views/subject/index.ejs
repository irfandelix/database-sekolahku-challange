<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/meta.ejs') %>
    <title>Halaman Subject</title>
  </head>
  <body>
    <div class="text-center">
      <h1>Data Subject</h1>
      <button class="btn btn-primary">
        <a href="tambahsubject" class="text-white">Tambah Subject</a>
      </button>
      <table class="table mt-4 mx-auto w-75">
        <thead>
          <tr>
            <th>id</th>
            <th>Name Subject</th>
            <th>ACTIONS</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach((subject) => { %>
            <tr>
              <% for(const key in subject){ %>
                <td> <%= subject[key] %></td>
              <% } %>
              <td><a href="/subject/edit/<%= subject.id%>">EDIT</a></td>
            </tr>
          <% } ) %>
        </tbody>
      </table>
    </div>
  </body>
</html>
